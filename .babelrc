{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "browsers": ["ie >= 11"]
                },
                "debug": true,
                "useBuiltIns": true
            }
        ],
        "react"
    ],
    "plugins": [
        [ "transform-object-rest-spread", { "useBuiltIns": true } ],
        "transform-class-properties"
    ],
    "sourceMaps": "inline"
}
